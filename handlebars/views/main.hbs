<h1>Lista de productos</h1>
<table>
    <tr>
        <th>Nombre</th>
        <th>Precio</th>
        <th>Imagen</th>
    </tr>
    {{#each products}}
    <tr>
        <td>{{this.title}}</td>
        <td>{{this.price}}</td>
        <td><img src={{this.thumbnail}} width="80" alt="img_product"></td>
    </tr>
    {{/each}}
</table>

<a href="/">Ir al formulario</a>